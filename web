      service3Title: document.getElementById("service-3-title"),
      service3Desc: document.getElementById("service-3-desc"),
      processTitle: document.getElementById("process-title"),
      footerText: document.getElementById("footer-text")
    };

    // Funci√≥n que aplica el config actual a la UI
    async function applyConfig(config) {
      const cfg = Object.assign({}, defaultConfig, config || {});

      // Fuente base y tama√±o proporcional
      const baseFont = cfg.font_family || defaultConfig.font_family;
      const baseSize = Number(cfg.font_size || defaultConfig.font_size);
      const fontStack = `${baseFont}, system-ui, -apple-system, BlinkMacSystemFont, "Segoe UI", sans-serif`;

      if (domRefs.root) {
        domRefs.root.style.fontFamily = fontStack;
        domRefs.root.style.fontSize = baseSize + "px";
        domRefs.root.style.backgroundColor = cfg.background_color;
      }

      // Escala de tama√±os de texto
      if (domRefs.heroTitle) {
        domRefs.heroTitle.style.fontFamily = fontStack;
        domRefs.heroTitle.style.fontSize = baseSize * 2 + "px";
        domRefs.heroTitle.style.color = cfg.text_color;
        domRefs.heroTitle.textContent = cfg.hero_title || defaultConfig.hero_title;
      }

      if (domRefs.heroSubtitle) {
        domRefs.heroSubtitle.style.fontFamily = fontStack;
        domRefs.heroSubtitle.style.fontSize = baseSize * 0.9 + "px";
        domRefs.heroSubtitle.style.color = cfg.text_color;
        domRefs.heroSubtitle.textContent = cfg.hero_subtitle || defaultConfig.hero_subtitle;
      }

      if (domRefs.heroCta) {
        domRefs.heroCta.style.backgroundColor = cfg.primary_action_color;
        domRefs.heroCta.style.color = "#020617";
        domRefs.heroCta.style.fontFamily = fontStack;
        domRefs.heroCta.style.fontSize = baseSize * 0.9 + "px";
        domRefs.heroCta.textContent = cfg.hero_cta_text || defaultConfig.hero_cta_text;
      }

      if (domRefs.servicesTitle) {
        domRefs.servicesTitle.style.fontFamily = fontStack;
        domRefs.servicesTitle.style.fontSize = baseSize * 1.3 + "px";
        domRefs.servicesTitle.style.color = cfg.text_color;
        domRefs.servicesTitle.textContent = cfg.services_title || defaultConfig.services_title;
      }

      if (domRefs.service1Title) {
        domRefs.service1Title.style.fontFamily = fontStack;
        domRefs.service1Title.style.fontSize = baseSize * 0.95 + "px";
        domRefs.service1Title.style.color = cfg.text_color;
        domRefs.service1Title.textContent = cfg.service_1_title || defaultConfig.service_1_title;
      }
      if (domRefs.service1Desc) {
        domRefs.service1Desc.style.fontFamily = fontStack;
        domRefs.service1Desc.style.fontSize = baseSize * 0.8 + "px";
        domRefs.service1Desc.textContent = cfg.service_1_desc || defaultConfig.service_1_desc;
      }

      if (domRefs.service2Title) {
        domRefs.service2Title.style.fontFamily = fontStack;
        domRefs.service2Title.style.fontSize = baseSize * 0.95 + "px";
        domRefs.service2Title.style.color = cfg.text_color;
        domRefs.service2Title.textContent = cfg.service_2_title || defaultConfig.service_2_title;
      }
      if (domRefs.service2Desc) {
        domRefs.service2Desc.style.fontFamily = fontStack;
        domRefs.service2Desc.style.fontSize = baseSize * 0.8 + "px";
        domRefs.service2Desc.textContent = cfg.service_2_desc || defaultConfig.service_2_desc;
      }

      if (domRefs.service3Title) {
        domRefs.service3Title.style.fontFamily = fontStack;
        domRefs.service3Title.style.fontSize = baseSize * 0.95 + "px";
        domRefs.service3Title.style.color = cfg.text_color;
        domRefs.service3Title.textContent = cfg.service_3_title || defaultConfig.service_3_title;
      }
      if (domRefs.service3Desc) {
        domRefs.service3Desc.style.fontFamily = fontStack;
        domRefs.service3Desc.style.fontSize = baseSize * 0.8 + "px";
        domRefs.service3Desc.textContent = cfg.service_3_desc || defaultConfig.service_3_desc;
      }

      if (domRefs.processTitle) {
        domRefs.processTitle.style.fontFamily = fontStack;
        domRefs.processTitle.style.fontSize = baseSize * 1.3 + "px";
        domRefs.processTitle.style.color = cfg.text_color;
        domRefs.processTitle.textContent = cfg.process_title || defaultConfig.process_title;
      }

      if (domRefs.footerText) {
        domRefs.footerText.style.fontFamily = fontStack;
        domRefs.footerText.style.fontSize = baseSize * 0.75 + "px";
        domRefs.footerText.textContent = cfg.footer_text || defaultConfig.footer_text;
      }

      // Colores generales (background y superficies) ‚Äì aplicados v√≠a clases y estilos inline
      document.documentElement.style.backgroundColor = cfg.background_color;
      document.body.style.backgroundColor = cfg.background_color;
    }

    // Inicializar Element SDK si estÔøΩÔøΩÔøΩÔøΩ disponible
    if (window.elementSdk && typeof window.elementSdk.init === "function") {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange: async (config) => {
          await applyConfig(config);
        },
        mapToCapabilities: (config) => {
          const cfg = Object.assign({}, defaultConfig, config || {});
          return {
            recolorables: [
              {
                get: () => cfg.background_color,
                set: (value) => {
                  cfg.background_color = value;
                  window.elementSdk.setConfig({ background_color: value });
                }
              },
              {
                get: () => cfg.surface_color,
                set: (value) => {
                  cfg.surface_color = value;
                  window.elementSdk.setConfig({ surface_color: value });
                }
              },
              {
                get: () => cfg.text_color,
                set: (value) => {
                  cfg.text_color = value;
                  window.elementSdk.setConfig({ text_color: value });
                }
              },
              {
                get: () => cfg.primary_action_color,
                set: (value) => {
                  cfg.primary_action_color = value;
                  window.elementSdk.setConfig({ primary_action_color: value });
                }
              },
              {
                get: () => cfg.secondary_action_color,
                set: (value) => {
                  cfg.secondary_action_color = value;
                  window.elementSdk.setConfig({ secondary_action_color: value });
                }
              }
            ],
            borderables: [],
            fontEditable: {
              get: () => cfg.font_family,
              set: (value) => {
                cfg.font_family = value;
                window.elementSdk.setConfig({ font_family: value });
              }
            },
            fontSizeable: {
              get: () => Number(cfg.font_size),
              set: (value) => {
                cfg.font_size = Number(value);
                window.elementSdk.setConfig({ font_size: Number(value) });
              }
            }
          };
        },
        mapToEditPanelValues: (config) => {
          const cfg = Object.assign({}, defaultConfig, config || {});
          return new Map([
            ["hero_title", cfg.hero_title],
            ["hero_subtitle", cfg.hero_subtitle],
            ["hero_cta_text", cfg.hero_cta_text],
            ["services_title", cfg.services_title],
            ["service_1_title", cfg.service_1_title],
            ["service_1_desc", cfg.service_1_desc],
            ["service_2_title", cfg.service_2_title],
            ["service_2_desc", cfg.service_2_desc],
            ["service_3_title", cfg.service_3_title],
            ["service_3_desc", cfg.service_3_desc],
            ["process_title", cfg.process_title],
            ["footer_text", cfg.footer_text]
          ]);
        }
      });

      // Aplicar el config inicial
      applyConfig(window.elementSdk.config || defaultConfig);
    } else {
      // Fallback si el SDK no est√° disponible (vista previa simple)
      applyConfig(defaultConfig);
    }

    // Evitar navegaci√≥n del formulario de contacto
    const contactForm = document.querySelector("#contact form");
    if (contactForm) {
      contactForm.addEventListener("submit", function (e) {
        e.preventDefault();
      });
    }
  </script>
